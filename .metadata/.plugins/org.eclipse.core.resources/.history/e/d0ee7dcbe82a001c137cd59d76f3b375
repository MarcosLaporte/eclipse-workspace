#ifndef PRODUCTOS_H_
#define PRODUCTOS_H_
#include <stdio.h>
#include <stdlib.h>

#define EMPTY 0
#define FULL 1


typedef struct{
	int idProducto;
	char descripcion[51];
	int nacionalidad;
	int tipo;
	float precio;
	int estado;
}sProduct;



#endif /* PRODUCTOS_H_ */
